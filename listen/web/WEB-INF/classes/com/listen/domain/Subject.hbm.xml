<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.listen.domain.Subject" table="subject">
        <id name="id">
            <generator class="native"></generator>
        </id>
        <property name="answer"/>
        <property name="optionA"/>
        <property name="optionB"/>
        <property name="optionC"/>
        <property name="optionD"/>
        <property name="sort"/>
        <property name="analysis"/>
        <many-to-one name="library" class="com.listen.domain.Library" column="libraryId" cascade="none"/>
    </class>
</hibernate-mapping>