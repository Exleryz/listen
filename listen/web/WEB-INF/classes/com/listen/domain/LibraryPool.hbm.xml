<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.listen.domain.LibraryPool" table="LibraryPool">
        <id name="id">
            <generator class="native"></generator>
        </id>
        <property name="grade"/>
        <property name="checkPoint"/>
        <property name="score"/>
        <property name="subjectCount"/>
        <set name="librarieSet" table="SysLibraryLibraryPool" inverse="true" cascade="none">
            <key column="lpid"></key>
            <many-to-many class="com.listen.domain.Library" column="libid"/>
        </set>
    </class>
</hibernate-mapping>