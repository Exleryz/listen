<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.listen.domain.Student" table="Student">
        <id name="id">
            <generator class="native"></generator>
        </id>
        <property name="name"/>
        <property name="account"/>
        <property name="password"/>
        <property name="grade"/>
        <property name="classify"/>
        <property name="currentCheck"/>
        <!-- cascade="save-update" -->
        <set name="sysStudentLibraryPoolSet" cascade="none">
            <key column="stuId"></key>
            <one-to-many class="com.listen.domain.SysStudentLibraryPool"></one-to-many>
        </set>
    </class>
</hibernate-mapping>